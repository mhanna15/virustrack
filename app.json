{
  "expo": {
    "name": "virustrack",
    "slug": "virustrack",
    "privacy": "unlisted",
    "platforms": [
      "ios"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/virustrack.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch"
        ]
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.minahanna.virustrack"
    }
  }
}
